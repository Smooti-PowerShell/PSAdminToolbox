							</TableColumnItem>
							<TableColumnItem>
								<Propertyname>OSArchitecture</Propertyname>
							</TableColumnItem>
							<TableColumnItem>
								<Propertyname>SerialNumber</Propertyname>
							</TableColumnItem>
						</TableColumnItems>
					</TableRowEntry>
				</TableRowEntries>
